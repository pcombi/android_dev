		// TODO Auto-generated method stub
		super.onCreate(savedInstanceState);
		setContentView(R.layout.photo);
		initialize();
	}
	
	private void initialize() {
		iv = (ImageView) findViewById(R.id.ivReturnedPic);
		b = (Button) findViewById(R.id.bSetWall);
		iv = (ImageView) findViewById(R.id.ivReturnedPic);
		
	}
}
